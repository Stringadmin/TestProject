{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/", "destination": "/test.html" },
    { "source": "/comfyui/:match*", "destination": "/app.js" }
  ],
  "static": {
    "directory": ".", 
    "include": ["**/*.html", "**/*.js", "**/*.json"] 
  }
}