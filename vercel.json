{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "uploads/(.*)",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/comfy/:path*",
      "destination": "https://comfyui.oopshub.cn/:path*"
    },
    {
      "source": "/comfyui/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/uploads/:path*",
      "destination": "/uploads/:path*"
    },
    {
      "source": "/:path.(html|js|css|png|jpg|jpeg|gif|ico|json)",
      "destination": "/:path.$1"
    },
    {
      "source": "/",
      "destination": "/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "COMFYUI_API_URL": "/comfy"
  }
}
