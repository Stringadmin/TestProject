{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/", "destination": "/test.html" },
    { "source": "/(index|test|more).html", "destination": "/:path" },
    { "source": "/comfyui/:path*", "destination": "/app.js" },
    { "source": "/:file*\\.(css|js|png|jpg|jpeg|gif|svg)", "destination": "/:file*" }
  ]
}