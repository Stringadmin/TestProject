{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "uploads/(.*)",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/comfyui/image-proxy",
      "destination": "/api/index.js"
    },
    {
      "source": "/:path.(html|js|css|png|jpg|jpeg|gif|ico|json)",
      "destination": "/:path.$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.js"  
    }
  ],
  "env": {
    "NODE_ENV": "production"
  
  },
  "headers": [
    {
      "source": "/comfy/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}